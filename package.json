{
  "name": "portfolio-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.8.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "npm run prestart:prod",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "wp:build": "webpack --config webpack.config.js",
    "wp:serve": "node --inspect dist/main.js"
  },
  "dependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/platform-socket.io": "^7.0.7",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.7",
    "@typegoose/typegoose": "^6.3.2",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongodb": "^3.3.10",
    "@types/mongoose": "^5.7.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/async": "^3.0.3",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "2.0.7",
    "@types/uuid": "^3.4.6",
    "async": "^3.2.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "helmet": "^3.21.2",
    "mongo": "^0.1.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.8.13",
    "morgan": "^1.9.1",
    "node-cmd": "^3.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.4",
    "thumbnail": "^0.1.9",
    "ts-node-dev": "^1.0.0-pre.44",
    "typeorm": "^0.2.20",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/testing": "^6.11.7",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "5.16.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
